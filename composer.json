{
    "name": "wdalmut/php-deb-packager",
    "description": "A simple debian packager for PHP applications",
    "version":"0.0.3",
    "type":"library",
    "keywords":["deb","packager","ubuntu","php-deb"],
    "time":"2012-07-31",
    "license":"MIT",
    "authors": [
        {
            "name":"Walter Dal Mut",
            "email":"walter.dalmut@gmail.com",
            "homepage":"http://walterdalmut.com",
            "role":"Developer"
        }
    ],
    "support": {
        "email":"walter.dalmut@gmail.com"
    },
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
